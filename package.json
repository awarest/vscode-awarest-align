{
  "name": "awarest-align",
  "displayName": "awarest-align",
  "description": "Code and syntax alignment helpers",
  "version": "0.0.1",
  "license": "https://github.com/awarest/vscode-awarest-align/blob/master/LICENSE",
  "bugs": {
    "url": "https://github.com/awarest/vscode-awarest-align/issues"
  },
  "homepage": "https://github.com/awarest/vscode-awarest-align/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/awarest/vscode-awarest-align"
  },
  "publisher": "awarest",
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onCommand:extension.awarestAlignWhitespace"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.awarestAlignWhitespace",
        "title": "Awarest Align Whitespace"
      }
    ],
    "keybindings": [
      {
        "command": "extension.awarestAlignWhitespace",
        "key": "meta+space"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "keywords": [
    "awarest",
    "align",
    "alignment",
    "format",
    "formatting",
    "whitespace"
  ]
}